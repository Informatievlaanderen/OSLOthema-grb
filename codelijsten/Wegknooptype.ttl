@prefix skos:      <http://www.w3.org/2004/02/skos/core#> .
@prefix dct:       <http://purl.org/dc/terms/> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .


<https://data.vlaanderen.be/id/conceptscheme/Wegknooptype>
   a skos:ConceptScheme ;
   dct:modified "2021-05-28" ;
   skos:definition "De aard van de wegknoop."@nl ;
   skos:prefLabel "Wegknooptype"@nl .

<https://data.vlaanderen.be/id/concept/Wegknooptype/EchteKnoop>
   a skos:Concept ;
   skos:prefLabel "Echte knoop"@nl ;
   skos:definition "Punt waar 2 wegverbindingen (WVB) elkaar snijden, minstens drie aansluitende wegverbindingen."@nl ;
   skos:notation "Echte knoop"@nl ;
   skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> ;
   skos:inScheme <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> .

<https://data.vlaanderen.be/id/concept/Wegknooptype/Schijnknoop>
   a skos:Concept ;
   skos:prefLabel "Schijnknoop"@nl ;
   skos:definition "Punt waar 2 wegverbindingen (WVB) elkaar raken (verandering straatnaam, verandering type verharding, einde projectzone, ...), slechts twee aansluitende wegverbindingen."@nl ;
   skos:notation "Schijnknoop"@nl ;
   skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> ;
   skos:inScheme <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> .

<https://data.vlaanderen.be/id/concept/Wegknooptype/Eindknoop>
   a skos:Concept ;
   skos:prefLabel "Eindknoop"@nl ;
   skos:definition "Het einde van een doodlopende wegcorridor, slechts een aansluitende wegverbinding."@nl ;
   skos:notation "Eindknoop"@nl ;
   skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> ;
   skos:inScheme <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> .

<https://data.vlaanderen.be/id/concept/Wegknooptype/Minirotonde>
   a skos:Concept ;
   skos:prefLabel "Minirotonde"@nl ;
   skos:definition "Een kruispunt dat zich in de realiteit voordoet als een rotonde maar niet voldoet aan de geometrische specificaties om opgenomen te worden als een echte rotonde (ringvormige geometrie)."@nl ;
   skos:notation "Minirotonde"@nl ;
   skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> ;
   skos:inScheme <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> .

<https://data.vlaanderen.be/id/concept/Wegknooptype/Keerlusknoop>
   a skos:Concept ;
   skos:prefLabel "Keerlusknoop"@nl ;
   skos:definition "Juist twee aansluitende wegverbindingen met dezelfde straatnaam; wegverbindingen die aan beide zijden begrensd worden door hetzelfde wegknooppunt worden met behulp van een extra wegknooppunt (= keerlusknoop) opgesplitst."@nl ;
   skos:notation "Keerlusknoop"@nl ;
   skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> ;
   skos:inScheme <https://data.vlaanderen.be/id/conceptscheme/Wegknooptype> .